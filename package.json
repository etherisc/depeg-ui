{
  "name": "depeg-interface",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "postinstall": "$npm_execpath run typechainDepegTypes && $npm_execpath run typechainRegistryTypes && $npm_execpath run typechainChainlinkTypes",
    "dev": "next dev",
    "prebuild": "./extract_version.sh",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "testLoop": "jest --watch",
    "test": "jest",
    "lint": "next lint",
    "typechainDepegTypes": "typechain --target ethers-v5 --out-dir src/contracts/depeg-contracts './node_modules/@etherisc/depeg-contracts/build/contracts/**/*json'",
    "typechainChainlinkTypes": "typechain --target ethers-v5 --out-dir src/contracts/chainlink-contracts './node_modules/@chainlink/contracts/abi/v0.8/AggregatorV3Interface.json'",
    "typechainRegistryTypes": "typechain --target ethers-v5 --out-dir src/contracts/registry-contracts './node_modules/@etherisc/registry-contracts/build/contracts/**/*json'"
  },
